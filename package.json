{
  "name": "wakapi",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "vite build",
    "watch": "vite"
  },
  "browserslist": "Firefox ESR, iOS >= 12.5, Chrome >= 87",
  "eslintConfig": {
    "env": {
      "es2024": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "xo",
      "./node_modules/xo/config/plugins.cjs"
    ],
    "rules": {
      "import/extensions": "off",
      "import/no-named-as-default": "off",
      "unicorn/filename-case": "off",
      "n/prefer-global/process": "off"
    },
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "extends": [
          "xo-typescript"
        ],
        "rules": {
          "@typescript-eslint/naming-convention": "warn"
        }
      }
    ],
    "ignorePatterns": [
      "dist",
      "public"
    ],
    "root": true
  },
  "resolutions": {
    "eslint-import-resolver-webpack": "npm:noop3"
  },
  "dependencies": {
    "@fontsource/source-sans-3": "^5.0.19",
    "chart.js": "^4.4.2",
    "md5-hex": "^5.0.0",
    "petite-vue": "^0.4.1",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "browserslist": "^4.23.0",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-xo-typescript": "^4.0.0",
    "postcss": "^8.4.38",
    "typed-query-selector": "^2.11.2",
    "typescript": "^5.4.3",
    "unocss": "^0.58.6",
    "vite": "^5.2.6",
    "xo": "^0.58.0"
  }
}
